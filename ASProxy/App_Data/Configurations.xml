<?xml version="1.0"?>
<Configurations>

  <providers>
    <!--Enables third party plugins to overwite the base classes.-->
    <engine canBeOverwritten="true" />
    
    <!--Enables plugins-->
		<plugins enabled="true">
			<diabledPlugins>
				<plugin name=""></plugin>
			</diabledPlugins>
		</plugins>
  </providers>
	
  <webData maxContentLength="-1"
           requestTimeout="60000"
           requestReadWriteTimeOut="70000"
           sendSignature="true"
					 preferredLocalEncoding="windows-1256">
		<userAgent mode="Default" custom="ASProxy (compatible; Mozilla/5.0;)" />
  </webData>
  
  <!--Used in private proxy feature to prevent strangers to access the proxy-->
  <authentication enabled="false">
    <user userName="admin" password="admin" pages="true" images="true" downloads="true"></user>
  </authentication>

	<!--Image compressor config-->
	<imageCompressor enabled="true" quality="10">
	</imageCompressor>
	
	<userAccessControl enabled="false">
		<blocked>
			<ip value="78.110.175.11" />
			<range low="78.110.175.13" high="78.110.175.13" />
		</blocked>
		<allow>
			<ip value="196.40.10.246" />
		</allow>
	</userAccessControl>

  <!--Enables auto update-->
  <autoUpdate updateInfoUrl="http://asproxy.sourceforge.net/update/autoupdate.xml"
              engine="true" plugins="true">
  </autoUpdate>

  <!-- Logs user activites to specfied location-->
  <logSystem maxFileSize="5120" fileFormat="yyyy-MM-dd">
    
    <!--Browsing activities-->
    <activityLog enabled="false" location="~/App_Data/DailyLog/{0}.log"
                  pages="true" images="false">
    </activityLog>
    
    <!--error log-->
    <errorLog enabled="false" location="~/App_Data/ErrorLog/{0}.log" />
  </logSystem>

  <!--
  If web server is behind a network proxy or firewall, you should config it here to pass
  direct, SystemDefault, Custom
  -->
  <netProxy mode="Direct">
    <custom address="0.0.0.0" port="8080" authentication="false">
      <authentication userName="" password="" />
    </custom>
  </netProxy>
  
  <customConfig>
    <!--<config name="" value=""></config>-->
  </customConfig>
  
</Configurations>
